<%= simple_form_for @cochera, :html => { :class => 'form-horizontal' } do |f| %>
  <%= f.input :name %>
  <%= error_span(@cochera[:name]) %>
  <%= f.input :description %>
  <%= error_span(@cochera[:description]) %>

  <%= f.button :submit, :class => 'btn-primary' %>
  <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                cocheras_path, :class => 'btn btn-default' %>
<% end %>
